{
  "name": "eqchecker",
  "displayName": "Eqchecker",
  "publisher": "CompilerAI",
  "description": "",
  "version": "0.0.5",
  "engines": {
    "vscode": "^1.54.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:eqchecker.helloWorld"
  ],
  "browser": "./dist/web/extension.js",
  "contributes": {
    "views": {
      "explorer": [
        {
          "type": "webview",
          "id": "eqchecker.eqcheckView",
          "name": "Equivalence checks"
        },
        {
          "id": "eqchecker.searchTreeView",
          "name": "Search Tree"
        }
      ]
    },
    "commands": [
      {
        "command": "eqchecker.checkEq",
        "title": "Check Equivalence"
      },
      {
        "command": "eqchecker.setServer",
        "title": "Configure Equivalence Server"
      },
      {
        "command": "eqchecker.logout",
        "title": "Logout of the Equivalence Server"
      }
    ]
  },
  "scripts": {
    "test": "vscode-test-web --browserType=chromium --extensionDevelopmentPath=. --extensionTestsPath=dist/web/test/suite/index.js",
    "pretest": "npm run compile-web",
    "vscode:prepublish": "npm run package-web",
    "compile-web": "webpack",
    "watch-web": "webpack --watch",
    "package-web": "webpack --mode production --devtool hidden-source-map",
    "lint": "eslint src --ext ts",
    "run-in-browser": "vscode-test-web --browserType=chromium --extensionDevelopmentPath=. ."
  },
  "devDependencies": {
    "@types/vscode": "^1.54.0",
    "@types/mocha": "^10.0.1",
    "eslint": "^8.28.0",
    "@typescript-eslint/eslint-plugin": "^5.45.0",
    "@typescript-eslint/parser": "^5.45.0",
    "mocha": "^10.1.0",
    "typescript": "^4.9.3",
    "@vscode/test-web": "^0.0.33",
    "ts-loader": "^9.4.1",
    "webpack": "^5.75.0",
    "webpack-cli": "^5.0.0",
    "@types/webpack-env": "^1.18.0",
    "assert": "^2.0.0",
    "process": "^0.11.10"
  },
  "dependencies": {
    "@hpcc-js/wasm": "^0.3.11",
    "d3": "^7.8.4",
    "d3-graphviz": "^3.0.5",
    "p5": "^1.4.1",
    "prismjs": "^1.29.0",
    "highlight.js": "^11.7.0",
    "vscode-tas-client": "^0.1.63"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/bsorav/vscode-extension"
  }
}
